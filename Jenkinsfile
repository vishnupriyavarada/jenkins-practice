pipeline{
    agent none
    stages{
        stage('build'){
            steps{
                script{
                    echo "Hi am from build"
                }
            }
        }
        stage('Test'){
            steps{
                script{
                    echo "Hi am from test"
                }
            }
        }
        stage('deploy'){
            steps{
                script{
                    echo "Hi am from deploy"
                }
            }
        }
    }
}